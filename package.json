  },
  "license": "MIT",
  "keywords": [
    "Next",
    "Next TopLoader",
    "Next.js",
    "Next.js 14",
    "progress bar app directory",
    "Next.js 13",
    "Nprogress",
    "React",
    "Top Loading Bar",
    "Progressbar",
    "next-app-progress-bar"
  ],
  "repository": {
    "type": "git",
    "directory": "https://github.com/siamahnaf/next-progress",
    "url": "https://github.com/siamahnaf/next-progress"
  },
  "devDependencies": {
    "@types/node": "^24.9.1",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "next": "^16.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "next": "^15 || ^16",
    "react": "^19",
    "react-dom": "^19"
  },
  "dependencies": {
    "nprogress": "^0.2.0"
  }
